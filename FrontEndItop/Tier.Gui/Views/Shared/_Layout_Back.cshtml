@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="es-co">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title | @Tier.Gui.Base.ApplicationConfigurationManager.ApplicationName</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="@Url.Content("~/bootstrap/css/bootstrap.min.css")">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/dist/css/AdminLTE.min.css")">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="@Url.Content("~/dist/css/skins/_all-skins.min.css")">
    <!-- iCheck -->
    <link rel="stylesheet" href="@Url.Content("~/plugins/iCheck/all.css")">
    <!-- Morris chart -->
    <link rel="stylesheet" href="@Url.Content("~/plugins/morris/morris.css")">
    <!-- jvectormap -->
    <link rel="stylesheet" href="@Url.Content("~/plugins/jvectormap/jquery-jvectormap-1.2.2.css")">
    <!-- Date Picker -->
    <link rel="stylesheet" href="@Url.Content("~/plugins/datepicker/datepicker3.css")">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="@Url.Content("~/plugins/daterangepicker/daterangepicker.css")">
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="@Url.Content("~/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css")">
    <!-- Select2 -->
    <link rel="stylesheet" href="@Url.Content("~/plugins/select2/select2.min.css")">
    <!-- Datatables -->
    <link href="@Url.Content("~/plugins/datatables/jquery.dataTables.min.css")" rel="stylesheet" />
    <!-- Pace -->
    <link href="@Url.Content("~/plugins/pace/pace.min.css")" rel="stylesheet" />

    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- jQuery 2.2.3 -->
    <script src="@Url.Content("~/plugins/jQuery/jquery-2.2.3.min.js")"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>

    <!-- Bootstrap 3.3.6 -->
    <script src="@Url.Content("~/bootstrap/js/bootstrap.min.js")"></script>
    <!-- Morris.js charts -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>

    <script src="@Url.Content("~/plugins/morris/morris.min.js")"></script>
    <!-- Sparkline -->
    <script src="@Url.Content("~/plugins/sparkline/jquery.sparkline.min.js")"></script>
    <!-- jvectormap -->
    <script src="@Url.Content("~/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js")"></script>

    <script src="@Url.Content("~/plugins/jvectormap/jquery-jvectormap-world-mill-en.js")"></script>
    <!-- jQuery Knob Chart -->
    <script src="@Url.Content("~/plugins/knob/jquery.knob.js")"></script>
    <!-- daterangepicker -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>

    <script src="@Url.Content("~/plugins/daterangepicker/daterangepicker.js")"></script>
    <!-- datepicker -->
    <script src="@Url.Content("~/plugins/datepicker/bootstrap-datepicker.js")"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="@Url.Content("~/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js")"></script>
    <!-- Slimscroll -->
    <script src="@Url.Content("~/plugins/slimScroll/jquery.slimscroll.min.js")"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/plugins/fastclick/fastclick.js")"></script>
    <!-- AdminLTE App -->
    <script src="@Url.Content("~/dist/js/app.min.js")"></script>
    <!-- Select2 -->
    <script src="@Url.Content("~/plugins/select2/select2.min.js")"></script>
    <!-- icheck -->
    <script src="@Url.Content("~/plugins/iCheck/icheck.min.js")"></script>
    <!-- Datatables -->
    <script src="@Url.Content("~/plugins/datatables/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/plugins/datatables/dataTables.bootstrap.min.js")"></script>
    <!-- Pace -->
    <script data-pace-options='{ "document": false, "elements": false, "eventLag": false, "ajax": false }' src='@Url.Content("~/plugins/pace/pace.min.js")'></script>
    <!-- jquery.validate -->
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
    <!-- jquery.unobtrusive -->
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")"></script>
    <!-- pnotify -->
    <link href="@Url.Content("~/plugins/notify/pnotify.custom.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/plugins/notify/pnotify.custom.min.js")"></script>
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index","BackEnd")" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>F-E</b> i</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Front-End</b> iTop</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Notifications: style can be found in dropdown.less -->
                        @Html.Partial("_TopBarNotifications")
                        <!-- User Account: style can be found in dropdown.less -->
                        @Html.Partial("_TopBarUserMenu")
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                @Html.Partial("_SideBarUserPanel")
                <!-- sidebar menu: : style can be found in sidebar.less -->
                @Html.Partial("_SideBarMenu")
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewBag.SectionTitle
                </h1>
                <ol class="breadcrumb">
                    <li><a href="@Url.Action("Index", "BackEnd")"><i class="fa fa-dashboard"></i> Inicio</a></li>
                    <li class="active">@ViewBag.Title</li>
                </ol>
            </section>
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0.0
            </div>
            <strong>Copyright &copy; 2016 <a href="http://www.kappa10.com/" target="_blank">Kappa10 Ltda.</a> - Desarrollado por <a href="http://www.netdroix.com.co" target="_blank">Netdroix Colombia</a>.</strong> Todos los derechos reservados.
        </footer>
    </div>
    <!-- ./wrapper -->
</body>
</html>
<script>
    PNotify.prototype.options.styling = "bootstrap3";
    $(document).ajaxStart(function () { Pace.restart(); });
</script>